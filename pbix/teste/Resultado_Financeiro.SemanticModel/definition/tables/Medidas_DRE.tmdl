	measure '% DRE' =
			
			var ReceitaSemFiltro = CALCULATE([Receita], ALL('D_PlanoDeContas'))
			RETURN DIVIDE([Valor_DRE], ReceitaSemFiltro, 0)
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: 17016560-5c7b-4e87-8d91-b259a0c7d301

