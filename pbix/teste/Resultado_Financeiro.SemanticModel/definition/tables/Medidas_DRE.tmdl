table Medidas_DRE
	lineageTag: 896d4dbe-0059-4ced-93ef-3c8834f296f6

	measure Valor_DRE = ```
			
			VAR Cod = SELECTEDVALUE(D_PlanoDeContas[DRE])
			RETURN
			    IF(Cod = "(+) FATURAMENTO BRUTO", [Receita],
			    IF(Cod = "(-) IMPOSTOS", CALCULATE([Impostos], ALL(D_PlanoDeContas)),
			    IF(Cod = "(=) FATURAMENTO LIQUIDO" && NOT(ISINSCOPE(F_Movimentacoes[Nome_Operacao])), CALCULATE([Faturamento_Liquido], ALL(D_PlanoDeContas)),
			    IF(Cod = "(-) DESPESA OPERACIONAL", [Despesas_Operacionais],
			    IF(Cod = "(-) DESPESAS OPERACIONAIS GERAIS", [Despesas_Operacionais_Gerais],
			    IF(Cod = "(=) RESULTADO OPERACIONAL" && NOT(ISINSCOPE(F_Movimentacoes[Nome_Operacao])), CALCULATE([Resultado_Operacional], ALL(D_PlanoDeContas)),
			    IF(Cod = "(-) DESPESAS ADMINISTRATIVAS", [Despesas administrativas],
			    IF(Cod = "(=) RESULTADO BRUTO" && NOT(ISINSCOPE(F_Movimentacoes[Nome_Operacao])), CALCULATE([Resultado Bruto], ALL(D_PlanoDeContas)),
			    IF(Cod = "(-) INVESTIMENTOS", [Investimentos],
			    IF(Cod = "(-) DESPESAS EXTRAS", [Despesas_Extras],
			    IF(Cod = "(=) RESULTADO FINANCEIRO" && NOT(ISINSCOPE(F_Movimentacoes[Nome_Operacao])), CALCULATE([Resultado_Financeiro], ALL(D_PlanoDeContas))
			    
			)))))))))))
			```
		lineageTag: 1db5d4a5-8467-4741-a93e-5e87244dab79

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Receita = CALCULATE(SUM(F_Movimentacoes[Valor_Total]), D_Categorias[Categoria_DRE] = "(+) FATURAMENTO BRUTO")
		lineageTag: abe80c93-1f7c-4ce2-b51b-515f008100a3

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Impostos = CALCULATE(SUM(F_Movimentacoes[Impostos]), D_Categorias[Categoria_DRE] = "(-) DESPESAS OPERACIONAIS GERAIS") *-1
		lineageTag: 5f3ae856-e7c2-40a7-8287-f7be73c4705f

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Faturamento_Liquido = [Receita] + [Impostos]
		lineageTag: 5450726c-bc04-4add-a2ee-9ae17a5d23df

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure Despesas_Operacionais = ```
			
