	measure TxtDiaAtual = ```
			
			var primeiroDia = MIN(D_Calendario[Date])
			var ultimoDia = MAX(D_Calendario[Date])
			return
			    IF(primeiroDia=ultimoDia,CONCATENATE("MOVIMENTAÇÃO DE ", ultimoDia),
			        CONCATENATE("MOVIMENTAÇÃO DE ",
			            CONCATENATE(primeiroDia,
			                CONCATENATE(" ATÉ ", ultimoDia))))
			
			```
		lineageTag: 394086c1-3b52-401a-9e70-7108de0a274c

	measure CorFundo = ```
			
			VAR vCor = MAX(D_PlanoDeContas[Cor])
			VAR Receita = "#B0E0E6"
			VAR vDefault = "DEFAULT"
			VAR vCorFundoReceita = "#D8BFD8"
			VAR vResultado = "#000000"
			return
			//1 = RESULTADOS
			//2 = RECEITA
			//3 = FATURAMENTO
			
			SWITCH(
			    TRUE(),
			    vCor = "1",Receita,
			    vCor = "2",vCorFundoReceita,
			    vCor = "3",vResultado,
			    vDefault
			    )
			
			```
		lineageTag: 7abcd6bd-d293-4cb8-b9d7-380313319ae9

	measure DataMaisRecente = ```
			
			    CALCULATE(MAX(D_Calendario[Date]), FILTER(F_Movimentacoes, F_Movimentacoes[Data]))
